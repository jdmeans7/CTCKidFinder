@model IEnumerable<Chirst_Temple_Kid_Finder.Models.CodeTable>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Generate new code","Generate","CodeTables")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ChildCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Room_Number)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ChildCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Room_Number)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            <!--@Html.ActionLink("Details", "Details", new { id=item.Id }) |-->
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
